{
  "compilerOptions": {
    "target": "ES2015",
    "module": "NodeNext",
    "declaration": true,
    "outDir": "./lib",
    "strict": true,
    "moduleResolution": "nodenext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "useUnknownInCatchVariables": false,
    "strictPropertyInitialization": false
  },
  "include": ["src"]
}
